<triggers>
  <trigger
   name="trg_startCapture"
   enabled="n"
   match="^(You echo )?-- START CAPTURE --"
   regexp="y"
   omit_from_output="y"
   omit_from_log="y"
   sequence="100"
   script="captureStart"
  >
  </trigger>
  <trigger
   name="trg_endStatCapture"
   enabled="y"
   match="^(You echo )?-- END CAPTURE --"
   regexp="y"
   omit_from_output="y"
   omit_from_log="y"
   sequence="100"
   script="captureEnd"
   group="tgrp_endCaptures"
  >
  </trigger>
  <trigger
   name="trg_endEditCapture"
   enabled="n"
   match="You are back in the game."
   regexp="y"
   omit_from_output="n"
   omit_from_log="n"
   sequence="100"
   script="captureEnd"
   group="tgrp_endCaptures"
  >
  </trigger>
  
  <trigger
   name="trg_captureLoopEnd"
   enabled="y"
   match="^(You echo )?-- END ALL CAPTURE COMMANDS --"
   regexp="y"
   omit_from_output="y"
   omit_from_log="y"
   sequence="100"
   script="captureLoopEnd"
  >
  </trigger>

